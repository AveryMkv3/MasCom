
<%- include('header') -%> 

<div class="container">
    <form action="/register" method="post">
     <div class="mb-3">
       <label for="name" class="form-label">Nom:</label>
       <input  autofocus type="text" id="name" name="name" class="form-control shadow-none mb-3" >
     </div>
     <div class="mb-3">
        <label for="lastname" class="form-label">Prenoms:</label>
        <input  autofocus type="text" id="lastname" name="lastname" class="form-control shadow-none mb-3" >
    </div>
    <div class="mb-3">
        <label for="username" class="form-label">Pseudo:</label>
        <input  autofocus type="text" id="username" name="username" class="form-control shadow-none mb-3" >
    </div>
     <div class="mb-3">
        <label for="email" class="form-label">Email:</label>
        <input  autofocus type="email" id="email" name="email" class="form-control shadow-none mb-3" >
      </div>
     <div class="mb-3">
      <label for="password" class="form-label">Mot de passe</label>
     <input autofocus type="password" name="passwordhash" class="form-control shadow-none mb-3" id="inputPassword" >
   </div>
   <div class="btt">
       <button type="submit" class="btn btn-primary">Ajouter</button>
   </div>
 </form>
</div> 


<%- include('footer') -%> 
